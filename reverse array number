import java.io.*;
import java.util.Arrays;
class reversearray{
    public static void main(String[] args){
        int temp=0;
        int[] arr ={4,2,5,6,7,8,9,0};
        for(int i=arr.length-1;i>=0;i--){
            arr[temp]=arr[i];
            temp++;
        }
            for(int n:arr){
                System.out.println(n+"");
            }
         
        }
    }
