import java.io.*;
class lcm{
    public static void main(String args[]){
        int a=50,b=60;
        int g=0;
        for(int i=1;i<=a;i++){
            if(a%i==0 && b%i==0)
                g=i;
            }
            int lcm=a*b/g;
            System.out.println("enter the value of lcm"+lcm);
        }
    }
